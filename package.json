{
  "name": "sf-catalog",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "migrate": "prisma migrate dev",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "NODE_ENV=development npm run migrate && tsx watch src/server.ts",
    "test": "jest",
    "seed": "ts-node src/database/seed.ts",
    "format": "prettier --write .",
    "static-analysis:lint": "eslint . --ext .ts",
    "static-analysis:typecheck": "tsc --noEmit",
    "static-analysis:format:check": "prettier --check ."
  },
  "dependencies": {
    "@faker-js/faker": "10.1.0",
    "@prisma/client": "6.2.1",
    "@rjrcosta/contracts": "^1.0.4",
    "@rjrcosta/messaging-rabbit": "^1.0.0",
    "amqplib": "^0.10.9",
    "axios": "1.13.2",
    "dotenv": "16.3.1",
    "express": "^4.22.1",
    "mysql2": "^3.15.3",
    "prisma": "6.2.1",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "tsx": "4.21.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/amqplib": "^0.10.8",
    "@types/express": "4.17.17",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "8.15.6",
    "@types/supertest": "6.0.3",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-security": "3.0.1",
    "jest": "^29.7.0",
    "prettier": "3.7.4",
    "supertest": "7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
